{
  "name": "jumbo-common",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "tslint": "tslint --fix -c tslint.json -r node_modules/tslint-microsoft-contrib/ -t verbose 'spec/**/*.ts' 'src/**/*.ts'",
    "ts": "tsc",
    "ts:watch": "tsc -w",
    "unit": "istanbul cover  --print=none --include-all-sources --root=./dist/src --dir=./coverage --report=json jasmine",
    "start": "node dist/src/index.js",
    "build": "npm run ts && npm run unit",
    "install": "tsc -d",
    "prets": "npm run clean; npm run tslint",
    "prets:watch": "npm run clean; npm run tslint",
    "postunit": "remap-istanbul -i ./coverage/coverage-final.json -o ./coverage/report -t html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bunyan": "^1.8.4",
    "@types/jasmine": "2.8.6",
    "@types/nock": "9.1.3",
    "@types/node": "8.10.9",
    "@types/request": "2.47.0",
    "@types/request-promise-native": "1.0.14",
    "request": "2.85.0",
    "request-promise-native": "1.0.5",
    "typescript": "2.8.3"
  },
  "devDependencies": {
    "istanbul": "0.4.5",
    "jasmine": "3.1.0",
    "nock": "9.2.5",
    "remap-istanbul": "0.11.0",
    "rimraf": "2.6.2",
    "source-map-support": "^0.5.5",
    "tslint": "5.9.1",
    "tslint-microsoft-contrib": "5.0.3"
  }
}
